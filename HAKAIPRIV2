--Valor
_G.AutoAttack = True
--Fun√ß√£o
function AutoAttack()
while _G.AutoAttack == true do
local args = {
    [1] = "Crane Kick Actual Kick"
}
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Attack"):FireServer(unpack(args))
wait()
end
end
